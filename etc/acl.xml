<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Catalog::catalog">
                    <resource id="Mikimpe_SyncWithWMS::sync"
                              title="WMS Product Sync"
                              translate="title"
                              sortOrder="100"/>
                    <resource id="Mikimpe_SyncWithWMS::sync_request_history"
                              title="WMS Product Sync Request History"
                              translate="title"
                              sortOrder="110"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Mikimpe_SyncWithWMS::config"
                                      title="WMS Product Sync Configuration"
                                      translate="title"
                                      sortOrder="300"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
